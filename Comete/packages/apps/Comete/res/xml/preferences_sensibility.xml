<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen
  xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:holo="http://schemas.android.com/apk/res-auto"
   xmlns:robobunny="http://robobunny.com"
   holo:title="sensibility">
        <PreferenceCategory holo:title="Aileron">            
            <info.alni.comete.android.SeekBarPreference
            	holo:key="aileronMoltDefaultKey"
                holo:title="Moltiply"
                holo:summary="Adjust moltiply factor"
                holo:defaultValue="@string/aileronMoltDefault"
                robobunny:max="4.0"
                robobunny:interval="0.01"
                robobunny:units=""                
            />
        </PreferenceCategory>
        <PreferenceCategory holo:title="Elevator">
             <info.alni.comete.android.SeekBarPreference
            	holo:key="elevatorMoltDefaultKey"
                holo:title="Moltiply"
                holo:summary="Adjust moltiply factor"
                holo:defaultValue="@string/elevatorMoltDefault"
                robobunny:max="4.0"
                robobunny:interval="0.01"
                robobunny:units=""                
            />
            
            <info.alni.comete.android.SeekBarPreference
            	holo:key="elevatorSubDefaultKey"
                holo:title="Subtract"
                holo:summary="Adjust moltiply factor"
                holo:defaultValue="@string/elevatorSubDefault"
                robobunny:min="-1"
                robobunny:max="1"
                robobunny:interval="0.01"
                robobunny:units=""                
            />
        </PreferenceCategory>
        <PreferenceCategory holo:title="Rudder">            
            <info.alni.comete.android.SeekBarPreference
            	holo:key="rudderMoltDefaultKey"
                holo:title="Moltiply"
                holo:summary="Adjust moltiply factor"
                holo:defaultValue="@string/rudderMoltDefault"
                robobunny:max="4.0"
                robobunny:interval="0.01"
                robobunny:units=""                
            />
        </PreferenceCategory>
</PreferenceScreen>
